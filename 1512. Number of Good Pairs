class Solution {
    public int numIdenticalPairs(int[] nums) {
        
        int h=nums.length;
        int count=0;
        for(int i=0;i<h-1;i++){
            for(int j=i+1;j<h;j++){
                if(nums[i]==nums[j]){
                    count++;
                }
            }
        }
       return count;
    }
}
